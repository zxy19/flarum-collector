{
    "name": "xypp/collector",
    "description": "Data Collector collects all kinds of data for other extension to use.",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2.0",
        "xypp/flarum-localize-date-lib": "*",
        "nxp/math-executor": "^2.3"
    },
    "authors": [
        {
            "name": "小鱼飘飘",
            "email": "z@xypp.cc",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xypp\\Collector\\": "src/"
        },
        "files": [
            "classMappings.php"
        ]
    },
    "extra": {
        "flarum-extension": {
            "title": "Collector",
            "category": "feature",
            "icon": {
                "name": "fas fa-digital-tachograph",
                "color": "#fff",
                "backgroundColor": "#bf360c"
            },
            "optional-dependencies": [
                "xypp/store",
                "flarum/likes",
                "michaelbelgium/flarum-discussion-views",
                "v17development/flarum-user-badges",
                "fof/best-answer",
                "flarum/tags"
            ]
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": true,
                "css": true,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": true,
                "bundlewatch": false,
                "backendTesting": true,
                "editorConfig": true,
                "styleci": true
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "xypp/store": "*",
        "flarum/likes": "*",
        "michaelbelgium/flarum-discussion-views": "^7.3",
        "v17development/flarum-user-badges": "^1.1",
        "fof/best-answer": "^1.4",
        "flarum/tags": "^1.8"
    }
}
